			[ManipulationGroups:Manipulation-Group-Loot] 
			[ManipulationProfiles:MES-Manipulation-MaxTurretRange]
 
			[InitializeStoreBlocks:true] 
			[ContainerTypesForStoreOrders:MES-ContainerTypes-MESStoreBuyComponents-1,MES-ContainerTypes-MESStoreBuyOre-1,MES-ContainerTypes-MESStoreBuyIngots-1]
			
			[RivalAiSpawn:false]
			
			[UseRandomBuilderFaction:true] 
			 
			[IgnoreCombatPhase:true]  
			  
			[ReplenishSystems:true] 
				
			[UseRivalAi:true]
			[RivalAiReplaceRemoteControl:true]

			</Description>
			<IsPirate>true</IsPirate>
			<Frequency>5.0</Frequency>
			<Prefabs>
				<Prefab SubtypeId="Abandoned Ore Processing Outpost">
					<Position>
						<X>0.0</X>
						<Y>32.0</Y>
						<Z>0.0</Z>
					</Position>
					<Speed>0</Speed>
					<Behaviour>BehaviorInstallationAtmosphere</Behaviour>
				</Prefab> 
			</Prefabs>
		</SpawnGroup> 
 
